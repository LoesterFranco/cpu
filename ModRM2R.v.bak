module ModRM2R(RM,mod,R);
input [2:0]RM;
input mod;
output reg [15:0]R;
always @ (RM)
if(mod) R=16'b0000000000000000;
else
begin
	case(RM)
		3'b000:R=16'b0000000100000100;
		3'b001:R=16'b0000000100000001;
		3'b010:R=16'b0000000000010100;
		3'b011:R=16'b0000000000010001;
		3'b100:R=16'b0000000000000100;
		3'b101:R=16'b0000000000000001;
		3'b110:R=16'b0000000000010000;
		3'b111:R=16'b0000000100000000;
	default:R=16'b000000000000000;
	endcase
end
